<div id="page">

    {% if page.header %}
        <section id="header">
            {{ page.header }}
        </section>
    {% endif %}

    <header>
    </header>
    <section class="main">
        <div class="content">
            {{ page.content }}

            {{ kint(sidebar_mpu) }}

        </div>
        {% if page.left %}
            <aside class="left">
                {% if sidebar_mpu %}
                    {{ sidebar_mpu }}
                {% endif %}
                {{ page.left }}
            </aside>
        {% endif %}
    </section>
    {% if page.footer %}
        <footer id="footer">
            {{ page.footer }}
        </footer>
    {% endif %}



</div>

